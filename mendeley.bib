@article{Haglund2011,
	abstract = {Intracellular pathogens subvert the host cell cytoskeleton to promote their own survival, replication, and dissemination. Study of these microbes has led to many discoveries about host cell biology, including the identification of cytoskeletal proteins, regulatory pathways, and mechanisms of cytoskeletal function. Actin is a common target of bacterial pathogens, but recent work also highlights the use of microtubules, cytoskeletal motors, intermediate filaments, and septins. The study of pathogen interactions with the cytoskeleton has illuminated key cellular processes such as phagocytosis, macropinocytosis, membrane trafficking, motility, autophagy, and signal transduction.},
