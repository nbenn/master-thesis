	issn = {0021-9533 (Print)},
	journal = {Journal of cell science},
	keywords = {Actins,Angiomatosis,Bacillary,Bacterial Adhesion,Bartonella henselae,Cell Adhesion,Cell Division,Cell Membrane,Cells,Confocal,Cultured,Electron,Endocytosis,Endothelium,Humans,Intercellular Adhesion Molecule-1,Mechanical,Microscopy,Microtubules,Organelles,Phosphotyrosine,Pseudopodia,Scanning,Stress,Umbilical Veins,Vascular,Virulence,analysis,chemistry,cytology,metabolism,microbiology,pathogenicity,physiology,ultrastructure},
	language = {eng},
