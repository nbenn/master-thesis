	month = {jul},
	number = {2},
	pages = {223--229},
	title = {Construction of Versatile High-Level Expression Vectors for \textit{Bartonella hen\-selae} and the Use of Green Fluorescent Protein as a New Expression Mar\-ker},
	url = {http://www.sciencedirect.com/science/article/pii/S0378111998003199},
	volume = {215},
	year = {1998}
}

