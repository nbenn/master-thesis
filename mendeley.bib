	month = {jan},
	number = {1},
	pages = {1162.1--1162.18},
	pmid = {25534632},
	title = {Simultaneous Analysis of Large-Scale RNAi Screens for Pathogen Entry},
	url = {http://www.biomedcentral.com/1471-2164/15/1162},
