@article{Suomalainen2013,
	abstract = {Endocytosis is the most prevalent entry port for viruses into cells, but viruses must escape from the lumen of endosomes to ensure that viral genomes reach a site for replication and progeny formation. Endosomal escape also helps viruses bypass endolysosomal degradation and presentation to certain Toll-like intrinsic immunity receptors. The mechanisms for cytosolic delivery of nonenveloped viruses or nucleocapsids from enveloped viruses are poorly understood, in part because no quantitative assays are readily available which directly measure the penetration of viruses into the cytosol. Following uptake by clathrin-mediated endocytosis or macropinocytosis, the nonenveloped adenoviruses penetrate from endosomes to the cytosol, and they traffic with cellular motors on microtubules to the nucleus for replication. In this report, we present a novel single-cell imaging assay which quantitatively measures individual cytosolic viruses and distinguishes them from endosomal viruses or viruses at the plasma membrane. Using this assay, we showed that the penetration of human adenoviruses of the species C and B occurs rapidly after virus uptake. Efficient penetration does not require acidic pH in endosomes. This assay is versatile and can be adapted to other adenoviruses and members of other nonenveloped and enveloped virus families.},
	author = {Suomalainen, Maarit and Luisoni, Stefania and Boucke, Karin and Bianchi, Sarah and Engel, Daniel A. and Greber, Urs F},
	doi = {10/bb4t},
	issn = {1098-5514},
	journal = {Journal of Virology},
	keywords = {Adenoviridae,Adenoviridae Infections,Adenoviridae Infections: metabolism,Adenoviridae Infections: virology,Adenoviridae: physiology,Biological Assay,Capsid Proteins,Capsid Proteins: metabolism,Cell Membrane,Cell Membrane: metabolism,Cell Membrane: virology,Clathrin,Clathrin: metabolism,Cytosol,Cytosol: metabolism,Cytosol: virology,Endocytosis,Endosomes,Endosomes: metabolism,Endosomes: virology,HeLa Cells,Humans,Hydrogen-Ion Concentration,Receptors,Virus,Virus Internalization,Virus: metabolism},
	month = {nov},
	number = {22},
	pages = {12367--12379},
	pmid = {24027314},
	title = {A Direct and Versatile Assay Measuring Membrane Penetration of Adenovirus in Single Cells.},
	url = {http://jvi.asm.org/cgi/content/long/87/22/12367},
	volume = {87},
	year = {2013}
}

@article{Taubenberger2006,
	abstract = {The "Spanish" influenza pandemic of 1918–1919, which caused 50 million deaths worldwide, remains an ominous warning to public health. Many questions about its origins, its unusual epidemiologic features, and the basis of its pathogenicity remain unanswered. The public health implications of the pandemic therefore remain in doubt even as we now grapple with the feared emergence of a pandemic caused by H5N1 or other virus. However, new information about the 1918 virus is emerging, for example, sequencing of the entire genome from archival autopsy tissues. But, the viral genome alone is unlikely to provide answers to some critical questions. Understanding the 1918 pandemic and its implications for future pandemics requires careful experimentation and in-depth historical analysis.},
	author = {Taubenberger, Jeffery and Morens, David M.},
	doi = {10/bb4v},
	journal = {Emerging Infectious Diseases},
	number = {1},
	pages = {15--22},
	title = {1918 Influenza: The Mother of All Pandemics},
	volume = {Volume 12},
	year = {2006}
}

