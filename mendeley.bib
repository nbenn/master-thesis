@article{Truttmann2011,
	abstract = {The VirB/D4 type IV secretion system (T4SS) of the bacterial pathogen Bartonella henselae (Bhe) translocates seven effector proteins (BepA-BepG) into human cells that subvert host cellular functions. Two redundant pathways dependent on BepG or the combination of BepC and BepF trigger the formation of a bacterial uptake structure termed the invasome. Invasome formation is a multi-step process consisting of bacterial adherence, effector translocation, aggregation of bacteria on the cell surface and engulfment, and eventually, complete internalization of the bacterial aggregate occurs in an F-actin-dependent manner. In the present study, we show that Bhe-triggered invasome formation depends on integrin-beta1-mediated signaling cascades that enable assembly of the F-actin invasome structure. We demonstrate that Bhe interacts with integrin beta1 in a fibronectin- and VirB/D4 T4SS-independent manner and that activated integrin beta1 is essential for both effector translocation and the actin rearrangements leading to invasome formation. Furthermore, we show that talin1, but not talin2, is required for inside-out activation of integrin beta1 during invasome formation. Finally, integrin-beta1-mediated outside-in signaling by FAK, Src, paxillin and vinculin is necessary for invasome formation. This is the first example of a bacterial entry process that fully exploits the bi-directional signaling capacity of integrin receptors in a talin1-specific manner.},
	author = {Truttmann, Matthias C. and Misselwitz, Benjamin and Huser, Sonja and Har\-dt, Wolf-Dietrich and Critchley, David R. and Dehio, Christoph},
	doi = {10/cs6263},
	issn = {1477-9137 (Electronic)},
	journal = {Journal of Cell Science},
	keywords = {Actins,Angiomatosis,Antigens,Bacillary,Bacterial Proteins,Bartonella henselae,CD29,Cell Line,Cell Membrane,Humans,Protein Binding,Protein Transport,Signal Transduction,Talin,genetics,metabolism,microbiology,pathogenicity,physiology},
	language = {eng},
	month = {nov},
	number = {21},
	pages = {3591--3602},
	pmid = {22045736},
	title = {\textit{Bartonella henselae} Engages Inside-Out and Outside-In Signaling by Integrin \textbeta$_1$ and Talin1 During Invasome-Mediated Bacterial Uptake.},
	volume = {124},
	year = {2011}
}

@article{Veiga2005,
	abstract = {The bacterial pathogen Listeria monocytogenes uses the surface protein InlB to invade a variety of cell types. The interaction of InlB with the hepatocyte growth-factor receptor, Met, is crucial for infection to occur. Remarkably, the ubiquitin ligase Cbl is rapidly recruited to InlB-activated Met. Recent studies have shown that ligand-dependent endocytosis of Met and other receptor tyrosine kinases is triggered by monoubiquitination of the receptor, a process that is mediated by Cbl. Here, we show that purified InlB induces the Cbl-dependent monoubiquitination and endocytosis of Met. We then demonstrate that the bacterium exploits the ubiquitin-dependent endocytosis machinery to invade mammalian cells. First, we show that L. monocytogenes colocalizes with Met, EEA1, Cbl, clathrin and dynamin during entry. Then, we assess the role of different proteins of the endocytic machinery during L. monocytogenes infection. Over-expression or down-regulation of Cbl, respectively, increases or decreases bacterial invasion. Furthermore, RNA interference-mediated knock-down of major components of the endocytic machinery (for example, clathrin, dynamin, eps15, Grb2, CIN85, CD2AP, cortactin and Hrs), inhibit bacterial entry, establishing that the endocytic machinery is key to the bacterial internalization process.},
	author = {Veiga, Esteban and Cossart, Pascale},
	doi = {10/ccszb6},
	issn = {1465-7392 (Print)},
	journal = {Nature Cell Biology},
	keywords = {Animals,Carrier Proteins,Clathrin,Clathrin-Coated Vesicles,Down-Regulation,Dynamins,Endocytosis,HeLa Cells,Humans,Listeria monocytogenes,Listeriosis,Mammals,Membrane Proteins,Proto-Oncogene Proteins,Proto-Oncogene Proteins c-cbl,Proto-Oncogene Proteins c-met,RNA Interference,Ubiquitin,Ubiquitin-Protein Ligases,Vesicular Transport Proteins,genetics,metabolism,microbiology,physiology},
	language = {eng},
	month = {sep},
	number = {9},
	pages = {894--900},
	pmid = {16113677},
	title = {\textit{Listeria} Hijacks the Clathrin-Depen\-dent Endocytic Machinery to Invade Mammalian Cells.},
	volume = {7},
	year = {2005}
}

