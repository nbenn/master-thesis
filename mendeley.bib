	journal = {Annual Review of Microbiology},
	number = {1},
	pages = {523--541},
	title = {{Interactions of the Human Pathogenic \textit{Brucella} Species with Their Hosts}},
	url = {http://dx.doi.org/10.1146/annurev-micro-090110-102905},
