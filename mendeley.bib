	volume = {18},
	year = {2008}
}

@article{Liaw2002,
	abstract = {Recently there has been a lot of interest in “ensemble learning” - methods that generate many classifiers and aggregate their results. Two well-known methods are boosting and bagging of classification trees. In boosting, successive trees give extra weight to points incorrectly predicted by earlier predictors. In the end, a weighted vote is taken for prediction. In bagging, successive trees do not depend on earlier trees - each is independently constructed using a bootstrap sample of the data set. In the end, a simple majority vote is taken for prediction.},
	author = {Liaw, Andy and Wiener, Matthew},
	journal = {R News},
